      "token_limit": 2200
    },
    "ContentAnalysisAgent": {
      "capabilities": ["content_analysis", "sentiment_analysis"],
      "token_limit": 1800
    },
    "ResearchAgent": {
      "capabilities": ["web_research", "technical_research", "market_research", "literature_review"],
      "token_limit": 3000
    },
    "CodeGenAgent": {
      "capabilities": ["code", "code_generation", "refactoring"],
